# Environment variables for ManipulatorAI
# Duplicate this file to .env and fill in your actual values.
# This file should be added to .gitignore to keep secrets out of version control.

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=""
AZURE_OPENAI_ENDPOINT=""
OPENAI_API_VERSION="2023-05-15"
OPENAI_MODEL_NAME="gpt-4"

# Database URLs
POSTGRES_URL="postgresql+asyncpg://user:password@localhost:5432/manipulator_db"
MONGO_URL="mongodb://localhost:27017/manipulator_conversations"

# Redis Configuration
REDIS_HOST="localhost"
REDIS_PORT="6379"

# Webhook Verification Tokens
FACEBOOK_VERIFY_TOKEN="your_facebook_verify_token"
INSTAGRAM_VERIFY_TOKEN="your_instagram_verify_token"
